<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
     <script src="https://cdn.jsdelivr.net/npm/@tailwindcss/browser@4"></script>
    <title>Form page</title>
</head>
<body>
    

    <section>
       <%dataSelected.forEach(row=>{%>


         <form action="/updated/<%=row.student_id%>" method="post" class="w-[70%] mx-auto border-2  rounded-xl flex flex-col py-4 px-8 gap-3 mt-13 ">
            <input class="border border-orange-500 rounded"  type="text"  name="names" value="<%=row.names%>">
            <input class="border border-orange-500 rounded"  type="text"  name="email" value="<%=row.email%>">
            <input class="border border-orange-500 rounded"  type="text"  name="age" value="<%=row.age%>">
            <input class="border border-orange-500 rounded"  type="text"  name="grade" value="<%=row.grade%>">

            <button class="border border-orange-500 rounded w-30 bg-orange-300 text-white" type="submit">Submit</button>
        </form>


       <%})%>
    </section>

</body>
</html>